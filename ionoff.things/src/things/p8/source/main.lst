....................     disable_interrupts(INT_TIMER0); 
*
10B0:  BCF    FF2.5
....................     ret=TickCount; 
10B2:  MOVFF  7C,241
10B6:  MOVFF  7B,240
....................     enable_interrupts(INT_TIMER0); 
10BA:  BSF    FF2.5
....................     return ret; 
10BC:  MOVLB  2
10BE:  MOVFF  240,01
10C2:  MOVFF  241,02
.................... } 
10C6:  MOVLB  0
10C8:  RETLW  00
