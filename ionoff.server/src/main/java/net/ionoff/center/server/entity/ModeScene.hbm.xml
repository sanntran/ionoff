<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="net.ionoff.center.server.entity.ModeScene" table="modes_scenes">
		
		<cache usage="read-write"/>
		
		<id name="id" type="long" column="id">
			<generator class="increment" />
		</id>
		
		<many-to-one name="mode" class="net.ionoff.center.server.entity.Mode" fetch="select">
            <column name="mode_id" not-null="true" />
        </many-to-one>
		
		<many-to-one name="zone" class="net.ionoff.center.server.entity.Zone" fetch="select">
            <column name="zone_id" not-null="true" />
        </many-to-one>
		
		<many-to-one name="scene" class="net.ionoff.center.server.entity.Scene" fetch="select">
            <column name="scene_id" not-null="false" />
        </many-to-one>
		
	</class>
</hibernate-mapping>