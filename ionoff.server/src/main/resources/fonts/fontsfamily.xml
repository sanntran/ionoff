<?xml version="1.0" encoding="UTF-8"?>
<fontFamilies>
    <fontFamily name="Arial">
        <!-- Normal Arial Font -->
        <normal><![CDATA[fonts/arial.ttf]]></normal>
        <!-- Bold Arial Font -->
        <bold><![CDATA[fonts/arialbd.ttf]]></bold>
        <!-- Italic Arial Font -->
        <italic><![CDATA[fonts/ariali.ttf]]></italic>
        <!-- Bolt italic Arial Font -->
        <boldItalic><![CDATA[fonts/arialbi.ttf]]></boldItalic>
        <!-- Is font is embedded in PDF -->
        <pdfEncoding><![CDATA[Identity-H]]></pdfEncoding>
       <pdfEmbedded><![CDATA[true]]></pdfEmbedded>
       <exportFonts>
          <export key="net.sf.jasperreports.html">Arial, Helvetica, SansSerif</export>
          <export key="net.sf.jasperreports.xhtml">Arial, Helvetica, SansSerif</export>
       </exportFonts>
    </fontFamily>
</fontFamilies>

