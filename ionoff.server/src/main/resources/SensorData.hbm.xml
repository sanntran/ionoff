<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    
	<class name="net.ionoff.center.server.entity.SensorData" table="sensors_data">
		
		<id name="id" type="long" column="id">
			<generator class="increment" />
		</id>
		
		<many-to-one name="sensor" class="net.ionoff.center.server.entity.Sensor" fetch="select">
            <column name="sensor_id" not-null="true" />
        </many-to-one>
		 
		<property name="time">
			<column name="time_" sql-type="datetime"/>
		</property>
		
		<property name="value">
			<column name="value_"/>
		</property>

		<property name="index">
			<column name="index_"/>
		</property>
	</class>
	
</hibernate-mapping>