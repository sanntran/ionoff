<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    
	<class name="net.ionoff.center.server.entity.DashboardDevice" table="dashboards_devices">
		
		<id name="id" type="long" column="id">
			<generator class="increment" />
		</id>
		
		<many-to-one name="dashboard" class="net.ionoff.center.server.entity.Dashboard" fetch="select">
            <column name="dashboard_id" not-null="true" />
        </many-to-one>
        
		<many-to-one name="device" class="net.ionoff.center.server.entity.Device" fetch="select">
            <column name="device_id" not-null="true" />
        </many-to-one>
        
	</class>
	
</hibernate-mapping>