<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
			 xmlns:g="urn:import:com.google.gwt.user.client.ui"
			 xmlns:m="urn:import:gwt.material.design.client.ui"
			 xmlns:i="urn:import:net.ionoff.center.client.ui">

	<g:HTMLPanel ui:field="root" addStyleNames="dashboard">
		<m:MaterialRow addStyleNames="header">
			<m:MaterialColumn grid="s12">
				<m:MaterialLink ui:field="lblTitle" iconType="DASHBOARD" addStyleNames="title" />
			</m:MaterialColumn>
		</m:MaterialRow>
		<m:MaterialRow ui:field="cardRow">
			<m:MaterialColumn grid="l4 m6 s6 col">
				<i:DashboardCard ui:field="cartAlert"/>
			</m:MaterialColumn>
			<m:MaterialColumn grid="l4 m6 s6 col">
				<i:DashboardCard ui:field="cartArea"/>
			</m:MaterialColumn>
			<m:MaterialColumn grid="l4 m6 s6 col">
				<i:DashboardCard ui:field="cartZone"/>
			</m:MaterialColumn>
			<m:MaterialColumn grid="l4 m6 s6 col">
				<i:DashboardCard ui:field="cartController"/>
			</m:MaterialColumn>
			<m:MaterialColumn grid="l4 m6 s6 col">
				<i:DashboardCard ui:field="cartSensor"/>
			</m:MaterialColumn>
			<m:MaterialColumn grid="l4 m6 s6 col">
				<i:DashboardCard ui:field="cartDevice"/>
			</m:MaterialColumn>
			<m:MaterialColumn grid="l4 m6 s6 col">
				<i:DashboardCard ui:field="cartScene"/>
			</m:MaterialColumn>
			<m:MaterialColumn grid="l4 m6 s6 col">
				<i:DashboardCard ui:field="cartSchedule"/>
			</m:MaterialColumn>
			<m:MaterialColumn grid="l4 m6 s6 col">
				<i:DashboardCard ui:field="cartMode"/>
			</m:MaterialColumn>
		</m:MaterialRow>
		<m:MaterialRow ui:field="sliceRow"> </m:MaterialRow>

	</g:HTMLPanel>

</ui:UiBinder>