<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
			 xmlns:g="urn:import:com.google.gwt.user.client.ui"
			 xmlns:m="urn:import:gwt.material.design.client.ui"
			 xmlns:i="urn:import:net.ionoff.center.client.ui">


	<ui:style>
		.root {

		}
	</ui:style>

	<g:HTMLPanel ui:field="root" addStyleNames="{style.root}">
		<m:MaterialRow ui:field="header" addStyleNames="header">
			<m:MaterialColumn grid="s12">
				<m:MaterialLink ui:field="lblTitle" iconType="DOMAIN" addStyleNames="title" />
			</m:MaterialColumn>
		</m:MaterialRow>
		<m:MaterialRow ui:field="content"> </m:MaterialRow>
	</g:HTMLPanel>

</ui:UiBinder>